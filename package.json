{
  "name": "zigsai-agent-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:agent": "node scripts/mastra-mock-server.js",
    "dev:agent:mock": "node scripts/mastra-mock-server.js",
    "dev:agent:original": "cross-env NODE_ENV=development PORT=4111 mastra dev",
    "dev:agent:fallback": "node scripts/run-mastra-direct.js",
    "dev:agent:win": "node scripts/mastra-mock-server.js",
    "dev:ui": "next dev --turbopack",
    "dev": "concurrently \"pnpm:dev:agent\" \"pnpm:dev:ui\"",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "build:ui": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "start:ui": "next start",
    "build:agent": "mastra build",
    "start:agent": "mastra start",
    "lint": "next lint",
    "build": "npm run build:agent && npm run build:ui",
    "start": "concurrently \"npm:start:agent\" \"npm:start:ui\"",
    "docker:build": "docker build -t web-researcher-agent .",
    "docker:run": "docker run -p 3000:3000 -p 4111:4111 web-researcher-agent"
  },
  "dependencies": {
    "@ag-ui/mastra": "0.1.0",
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/openai": "^2.0.42",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-textarea": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@libsql/client": "^0.15.15",
    "@mastra/client-js": "0.14.1",
    "@mastra/core": "0.19.1",
    "@mastra/libsql": "0.15.0",
    "@mastra/mcp": "^0.13.4",
    "@mastra/memory": "0.15.4",
    "dotenv": "^17.2.3",
    "libsql": "^0.5.22",
    "mastra": "0.13.4",
    "next": ">=15.5.4",
    "ollama-ai-provider-v2": "^1.5.0",
    "openai": "^6.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
